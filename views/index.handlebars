<div class = "burger">
<h1>Log that Burger!</h1>
<img src = "data:image/jpeg;base64," alt = "burger" class = "burgers_r_us">
</div>
<br>
<div class="container">
  <div class = "row">
    <div class = "burgers_to_eat col-md-4">
      <h2 style="text-align: center">Burgers to Nom Nom!</h2>

        <ul>
          {{#each burgers}}
           {{#unless devoured}}
            {{> burgers/burger-block devoured=true}}
           {{/unless}}
          {{/each}}
        </ul>
    </div>

    <div class = "burgers_ate col-md-4">
      <h2 style="text-align:center">That burger was good!</h2>
        <ul>
          {{#each burgers}}
            {{#if devoured}}
              {{> burgers/burger-block devoured=false}}
            {{/if}}
          {{/each}}
        </ul>
    </div>
    
      <div class = "add_burger col-md-4">
        <h2 style="text-align: center">Create Burger!</h2>
          <form class="create-form">
            <div class="form-group">
              <label for="ca">Burger Name:</label>
              <input type="text" id="ca" name="name">
              <button type="submit">Add Burger</button>
            </div>
          </form>
      </div>
  </div>
</div>